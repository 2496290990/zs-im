<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.im.mapper.MyFriendMapper">

    <select id="queryMyFriendByUserId" resultType="com.im.entity.MyFriend">
        select *
        from im_my_friend
        when my_id = #{id}
    </select>
</mapper>
